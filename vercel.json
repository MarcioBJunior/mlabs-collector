{
  "version": 2,
  "functions": {
    "api/collect.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/collect",
      "schedule": "0 9 * * *"
    }
  ],
  "buildCommand": null,
  "outputDirectory": null,
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "false",
    "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
  }
}

